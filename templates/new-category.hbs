<h1>New Category</h1>
  <form>
    <div class="form-group">
      <label for="name">Enter new category title</label>
      {{input value=name id="name" class="form-control"}}
    </div>
    <button class="btn btn-primary" {{action 'save'}}>Save</button>
  </form>

{{outlet}}
<div>
  <br />
  {{#link-to 'categories' classNames="btn btn-warning"}}See list of Categories{{/link-to}}
  {{#link-to 'country' classNames="btn btn-warning"}}Go back to country{{/link-to}}

</div>
