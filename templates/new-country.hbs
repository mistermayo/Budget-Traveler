<h1>New Country</h1>
  <form>
    <div class="form-group">
      <label for="name">Enter new country</label>
      {{input value=name id="name" class="form-control"}}
    </div>
    <div class="form-group">
    <label for="image">Image:</label>
    {{input value=image id="image" class="form-control"}}
    </div>
    <button class="btn btn-primary" {{action 'save'}}>Save</button>
  </form>

{{outlet}}
<div>
  <br />
  {{#link-to 'countries' classNames="btn btn-warning"}}Go back to countries{{/link-to}}

</div>
